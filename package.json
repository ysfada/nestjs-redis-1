    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/ioredis": "^4.0.14",
    "@types/node": "^10.14.13",
    "@types/uuid": "^3.4.5",
    "cz-conventional-changelog": "^3.0.2",
    "rimraf": "^2.6.3",
    "typescript": "^3.5.3"
  },
  "keywords": [
    "nestjs",
    "nest",
    "redis",
    "nestjs-redis"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
