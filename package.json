{
  "name": "nestjs-redis",
  "version": "1.2.1",
  "description": "a nestjs redis module",
  "author": "zzy",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kyknow/nestjs-redis"
  },
